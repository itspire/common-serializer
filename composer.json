{
    "name": "itspire/serializer-common",
    "type": "library",
    "description": "Itspire Serializer - Common Classes",
    "keywords": ["Itspire", "Serializer"],
    "homepage": "http://www.itspire.fr",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ryan RAJKOMAR",
            "email": "ryan.rajkomar@itspire.fr"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/collections": "~1.7",
        "jms/serializer": "~3.9",
        "psr/http-message": "~1.0",
        "symfony/translation": "~4.4",
        "itspire/common": "dev-development"
    },
    "autoload": {
        "psr-4": {
            "Itspire\\Serializer\\Common\\": "src/main/php/"
        }
    },
    "require-dev": {
        "ext-simplexml": "*",
        "pdepend/pdepend": "~2.8",
        "phploc/phploc": "~7.0",
        "sebastian/phpcpd": "~6.0",
        "squizlabs/php_codesniffer": "~3.5",
        "symfony/phpunit-bridge": "~5.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Itspire\\Serializer\\Common\\Test\\": "src/test/php/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
